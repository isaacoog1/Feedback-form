{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/store-data",
            "dest": "index.js"
          },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}